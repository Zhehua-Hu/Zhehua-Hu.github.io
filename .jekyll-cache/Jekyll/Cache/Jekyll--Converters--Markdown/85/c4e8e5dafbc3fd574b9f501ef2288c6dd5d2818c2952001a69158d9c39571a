	<meta charset="utf-8" />
    <script type="text/javascript" src="http://zhehua.info/public/js/jquery.js"></script>
  <script type="text/javascript" src="http://zhehua.info/public/js/jquery.interactive_3d.js"></script>
  <style>
    body {
      text-align: center;
    }
	</style>
	<script>
	  $(document).ready( function() {
	    $("#i3d").interactive_3d({
	      frames: 38
	    });
	  });

	</script>
